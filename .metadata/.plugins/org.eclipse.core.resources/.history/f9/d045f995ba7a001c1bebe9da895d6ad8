package taulukko;

public class Etunimet {

	public static void main(String[] args) {

		String[] tiedot = 
				{ "Maria,1077", "Sofia,1030", "Aurora,869", "Olivia,760",
				"Juhani,1001", "Olavi,987", "Johannes,974", "Mikael,720" };

		for (int i = 0; i < tiedot.length; i++) {
			String[] tieto = tiedot[i].split(",");
			System.out.println(tieto[0] + tieto[1] + " kpl");
			
		}
	}

}
